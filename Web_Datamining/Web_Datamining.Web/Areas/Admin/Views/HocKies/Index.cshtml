@model IEnumerable<Web_Datamining.Models.HocKy>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Học Kì</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NamHoc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KyHoc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NamHoc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KyHoc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_HocKi }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_HocKi }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_HocKi })
        </td>
    </tr>
}

</table>
