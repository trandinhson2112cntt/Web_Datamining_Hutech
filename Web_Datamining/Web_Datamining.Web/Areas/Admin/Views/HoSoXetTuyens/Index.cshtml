                @Html.DisplayFor(modelItem => item.DanToc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TinhTrangTrungTuyen)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.MaHoSo }) |
                @Html.ActionLink("Details", "Details", new { id = item.MaHoSo }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.MaHoSo })
            </td>
        </tr>
    }

</table>
