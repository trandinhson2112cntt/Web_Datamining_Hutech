}

<h2>Hồ Sơ Xét Tuyển</h2>
@Html.Partial("~/Areas/Admin/Views/ImportExcel/_ImportHoSoXetTuyen.cshtml")
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DiemXetTuyen.DXT_ID)
