@model IEnumerable<Web_Datamining.Models.NganhTheoBo>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Ngành Theo Bộ</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TeNganh)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TeNganh)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaNganh }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaNganh }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaNganh })
        </td>
    </tr>
}

</table>
